  <!--
  修改這個檔案, 不會立即生效, 要重新啟動 IIS 服務或執行 http://server.ip/REST/ReloadConfig 才會重新載入
  REST API 定義:
    id: REST API的編號, 不可重覆
    name: REST API名稱
    sql: REST API的查詢來源, 值為 SqlMap 的 statement id (要含 namespace)
    type: (可省略)REST API的交易類型: select(預設), insert, update, delete
  -->
  <REST id="8818R" sql="REST.8818R" name="檢定成績統計(範例)" type="select">
    <!-- 
      <query> 定義REST API的查詢條件: 
        id: 在REST API送出查詢後, 會作為傳給 SqlMap 的 Parameters, 一律以全大寫的英數字定義
        name: 查詢欄位的名稱
        size: 設定條件欄位輸入值的長度限制.
        required: 條件欄位是否為必填(Y/N)
      -->
    <query>
      <condition type="text" id="EXAMKIND" name="檢定類別" size="2" required="Y"></condition>
      <condition type="text" id="YR" name="年度" size="4" required="Y"></condition>
      <condition type="text" id="STP" name="梯次" size="3" required="Y"></condition>
      <condition type="text" id="PNO" name="職類代碼" size="5"></condition>
    </query>
    
    <!--
      <header> 定義 REST API的輸出表頭區 欄位列表
    -->
    <header sql="REST.8818R_header">
      <column id="TITLE" name="標題" type="label" value="檢定成績統計"></column>
      <column id="EXAMKIND_TEXT" name="檢定類別名稱"></column>
    </header>
    <!-- 
      <layout><column> 定義REST API的輸出欄位列表 : 
        id: 欄位名稱, 值對應到 SqlMap 的結果欄位, 一律以全大寫英數字表示
        name: 欄位的標題名稱
        type: 欄位類型: "label" 或 未指定(資料欄位)
        format: 指定欄位值的輸出格式, 依欄位資料型別, 使用對應的格式字串,
                如: 
                Decimail數值可設定為: #.00
                DateTime數值可設定為: yyyy-mm-dd 
    -->
    <layout>
      <column id="OPCHNF" name="辦理單位名稱"></column>
      <column id="PNAME" name="職類"></column>
      <column id="EGRN" name="級別"></column>
      <column id="ASK1" name="分數1" format="#.##"></column>
      <column id="ASK2" name="分數2" format="#.##"></column>
      <column id="ASK3" name="分數3" format="#.##"></column>
      <column id="ASK4" name="分數4" format="#.##"></column>
      <column id="ASK5" name="分數5" format="#.##"></column>
    </layout>
  </REST>

