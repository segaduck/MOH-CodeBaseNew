@using EECOnline.Areas.SHARE.Models;
@model GRPFormModel

@if (Model.Grid != null)
{
    for (int i = 0; i < Model.Grid.Count; i++)
    {
        var item = Model.Grid[i];
        <tr>
            <td align="center"><button type="button" class="btn btn-info" onclick="doSelect('@item.grp_id','@item.grpname')">選</button></td>
            <td>@item.grp_id</td>
            <td>@item.grpname</td>
        </tr>
    }
}