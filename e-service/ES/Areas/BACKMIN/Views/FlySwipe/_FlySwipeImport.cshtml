
@using (Html.BeginForm("FlySwipeImport", "FlySwipe", FormMethod.Post, new { ID = "ActionForm2", enctype = "multipart/form-data" }))
{
    <div class="col-sm-12 content">
        <h1 class="main-title"><i class="fas fa-edit"></i>檔案上傳</h1>
        <div class="form-blue form-horizontal">
            <div class="form-group">
                <label class="main-label col-sm-2" for="">銀行別</label>
                <div class="col-sm-10">
                    <input type="radio" name="banktype" value="1" />中國信託
                    <input type="radio" name="banktype" value="2" />玉山銀行
                </div>
            </div>
            <div class="form-group">
                <label class="main-label col-sm-2" for="">選擇檔案</label>
                <div class="col-sm-10">
                    <input type="file" id="Text_UploadFile" name="Text_UploadFile" value="" class="form-control" accept=".csv" />
                </div>
            </div>
            <div class="btnbar">
                <input type="submit" value="上傳" class="btn btn-blue" />
            </div>
            <div class="form-group">
                <label class="main-label col-sm-2" for=""></label>
                <div class="col-sm-10">
                    @ViewBag.tempMessage
                </div>
            </div>
        </div>
    </div>
}
