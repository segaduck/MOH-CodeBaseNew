@using ES.Areas.Admin.Models;
@using ES.Services;

@model Apply_011003ViewModel

@{
    Layout = "~/Areas/BACKMIN/Views/Shared/_Layout.cshtml";
    ES.Models.ShareCodeListModel sc = new ES.Models.ShareCodeListModel();
    ViewBag.Title = "社會工作實務經驗年資審查線上申辦";
}

@using (Html.BeginForm("Index", "Apply_011003", FormMethod.Post, htmlAttributes: new { @class = "form-horizontal", id = "main_form", @enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(m => m.Form.SEX_CD)
    @Html.HiddenFor(m => m.Form.ISMEET)
    @Html.HiddenFor(m => m.Form.EMAIL)
    @Html.HiddenFor(m => m.Form.EDUCATION)
    @Html.HiddenFor(m => m.Form.MERGEYN)
    @Html.HiddenFor(m => m.Form.APP_ID)
    @Html.HiddenFor(m => m.Form.IsNotice)
    @Html.HiddenFor(m => m.Form.NAME)
    @Html.HiddenFor(m => m.Form.APP_TIME)
    <div class="bread">
        <i class="fas fa-home"></i>首頁 / 案件分文及處理 / 案件處理 / @ViewBag.Title
    </div>
    <div class="col-sm-12 content">
        <!-- H1 title -->
        <h1 class="main-title"><i class="fas fa-edit"></i>案件審理作業</h1>

        <div class="table-responsive">
            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="table-typeA table">
                <thead>
                    <tr>
                        <th colspan="2">基本資料檔</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td width="20%">
                            <div align="center">申辦項目：</div>
                        </td>
                        <td width="80%">
                            <div align="left">社會工作實務經驗年資審查線上申辦</div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">申辦日期：</div>
                        </td>
                        <td>@Model.Form.APP_TIME</td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">預計完成日：</div>
                        </td>
                        <td>@Model.Form.APP_EXT_DATE</td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">本部案件承辦人姓名：</div>
                        </td>
                        <td>@Model.Form.PRO_NAM</td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">公文文號：</div>
                        </td>
                        <td>@Model.Form.MOHW_CASE_NO</td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">案件編號：</div>
                        </td>
                        <td>@Model.Form.APP_ID</td>
                    </tr>
                    <tr>
                        <td>
                            <div align="center">系統狀態：</div>
                        </td>
                        <td>@Model.Form.APP_STATUS</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <!--1-->
        <div class='panel panel-info'>
            <div class='panel-heading'>
                <h2 class='panel-title' data-toggle='collapse' data-parent='#basicData' href='#basicData'>
                    <a>基本資料檔(點擊展開)</a>
                </h2>
            </div>
            <div id='basicData' class='panel-collapse collapse'>
                <div class='panel-body'>
                    <div class="form-blue">
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">申辦項目</label>
                            <div class="col-sm-4">
                                <p class="form-control-static">社會工作實務經驗年資審查線上申辦</p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">申請種類：依「專門職業及技術人員高等考試社會工作師考試規則」第六條申請部分科目免試</label>
                            <div class="col-sm-10">
                                <label class="radio-inline "><input type="radio" name="ISMEET" value="1" onchange="RadioISMEETChange()" disabled>符合第一款(國內社會工作實務經驗五年以上，有中央主管機關審查合格之證明文件。)</label>
                                <label class="radio-inline "><input type="radio" name="ISMEET" value="2" onchange="RadioISMEETChange()" disabled>符合第二款(外國社會工作師證書及國內社會工作實務經驗一年以上，有中央主管機關審查合格之證明文件。)</label>
                                <label class="radio-inline "><input type="radio" name="ISMEET" value="3" onchange="RadioISMEETChange()" disabled>符合第三款(曾任公立或依法立案之私立專科以上學校講師三年以上、助理教授或副教授二年以上、教授一年以上，<br /> &nbsp;&nbsp;講授前條第一項所列學科至少二科，並有國內社會工作實務經驗一年以上，有中央主管機關審查合格之證明文件。)</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">姓名</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">@Model.Form.NAME</p>
                                @* @Html.TextBoxFor(m => m.Form.NAME, new { @id = "NAME", @class = "form-normal", placeholder = "", @readonly = true })*@
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">出生年月日</label>
                            <div class="col-sm-4">
                                <div class="input-group">
                                    <p class="form-control-static">@Model.Form.BIRTHDAY</p>
                                    @*@Html.DatePickerTWFor(m => m.Form.BIRTHDAY, new { @readonly = true, disabled = "disabled" })*@
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">國民身分證統一編號</label>
                            <div class="col-sm-4">
                                <p class="form-control-static">@Model.Form.IDN</p>
                                @*@Html.TextBoxFor(m => m.Form.IDN, new { @id = "IDN", @class = "form-normal", placeholder = "", @readonly = true })*@
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">性別</label>
                            <div class="col-sm-4">
                                <label class="radio-inline "><input type="radio" value="M" name="SEX_CD" onchange="RadioSEXChange()" disabled>男</label>
                                <label class="radio-inline "><input type="radio" value="F" name="SEX_CD" onchange="RadioSEXChange()" disabled>女</label>
                                <label class="radio-inline "><input type="radio" value="O" name="SEX_CD" onchange="RadioSEXChange()" disabled>其他</label>
                            </div>
                        </div>
                        <!--網站電話-->
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">電話(公)</label>
                            <div class="col-sm-10">
                                @Html.TelForTurbo(m => m.Form.TEL)
                                @*<span>(</span>
                                    @Html.TextBoxFor(m => m.Form.TEL, new { @id = "TEL", @class = "form-10 form-normal", placeholder = "", @readonly = true })
                                    <span>)</span>
                                    @Html.TextBoxFor(m => m.Form.TEL_0, new { @id = "TEL_0", @class = "form-normal", placeholder = "", @readonly = true })
                                    <span>#</span>
                                    @Html.TextBoxFor(m => m.Form.TEL_1, new { @id = "TEL_1", @class = "form-normal", placeholder = "", @readonly = true })*@
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">電話(宅)</label>
                            <div class="col-sm-10">
                                @Html.TelForTurbo(m => m.Form.FAX)
                                @*<span>(</span>
                                    @Html.TextBoxFor(m => m.Form.FAX, new { @id = "FAX", @class = "form-10 form-normal", placeholder = "", @readonly = true })
                                    <span>)</span>
                                    @Html.TextBoxFor(m => m.Form.FAX_0, new { @id = "FAX_0", @class = "form-normal", placeholder = "", @readonly = true })
                                    <span>#</span>
                                    @Html.TextBoxFor(m => m.Form.FAX_1, new { @id = "FAX_1", @class = "form-normal", placeholder = "", @readonly = true })*@
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">行動電話</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Form.MOBILE, new { @id = "MOBILE", @class = "form-normal", placeholder = "", @readonly = true })
                            </div>
                        </div>
                        <!--網站地址-->
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">通訊地址<br />(含郵遞區號，請填目前服務單位或可收掛號信件之地址)</label>
                            <div class="col-sm-10 form-inline">
                                @Html.TextBoxFor(m => m.Form.ADDR, new { @id = "ADDR", @class = "form-10 form-normal", size = 10, placeholder = "郵遞區號", @readonly = true })
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Form.ADDR_TEXT, new { @id = "ADDR_TEXT", @class = "form-normal", size = 20, placeholder = "郵遞區號名稱", @readonly = "readonly" })
                                    <span class="input-group-btn">
                                        <button type="button" class="btn btn-info" onclick="doACC_ADDRSelect()" disabled><i class="fa fa-search" aria-hidden="true"></i></button>
                                    </span>
                                </div>
                                @Html.TextBoxFor(m => m.Form.ADDR_DETAIL, new { @id = "ADDR_DETAIL", @class = "form-normal", placeholder = "請輸入地址 ", size = 59, maxlength = "64", @readonly = true })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">E-MAIL</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Form.MAIL, new { @id = "MAIL", @class = "form-normal", @readonly = true })
                                @@
                                @Html.DropDownListFor(m => m.Form.MAIL_0, sc.GetMailDomainList, new { @class = "form-normal", disabled = "disabled" })
                                @Html.TextBoxFor(m => m.Form.MAIL_1, new { @id = "MAIL_1", @class = "form-normal", @readonly = true })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">學歷證明</label>
                            <div class="col-sm-10">
                                <label class="radio-inline "><input type="radio" name="EDUCATION" value="1" onchange="doEDUCATIONChange()" disabled>1.中華民國一百零二年起，經考選部依「<a href="https://wwwc.moex.gov.tw/main/ExamLaws/wfrmExamLaws.aspx?kind=3&menu_id=320&laws_id=88" target="_blank">專技人員社會工作師考試應考資格第五條審議通過並公告名單</a>」所列學校之畢業證書影本</label><br />
                                <label class="radio-inline "><input type="radio" name="EDUCATION" value="2" onchange="doEDUCATIONChange()" disabled>2.非考選部公告之學校系所(學分證明) (如檢附成績單，請清楚標記15學科(如螢光筆劃記)，以利審查)</label><br />
                                <label class="radio-inline "><input type="radio" name="EDUCATION" value="3" onchange="doEDUCATIONChange()" disabled>3.經教育部承認之國外專科以上社會工作相關科、系、組、所、學位學程畢業證書影本</label><br />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">學歷-學校名稱<br>(請填全銜)</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Form.SCHOOL, new { @id = "SCHOOL", @class = "form-normal", @readonly = true })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">學歷-系(組)所名稱</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Form.OFFICE, new { @id = "OFFICE", @class = "form-normal", @readonly = true })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">學歷-畢業年月</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">
                                    @Model.Form.GRADUATION
                                    <label>年</label>
                                    @Model.Form.GRADUATION_MONTH
                                    <label>月</label>
                                </p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="main-label col-sm-2" for="">考試通知書-通知年月</label>
                            <div class="col-sm-10">
                                @if (Model.Form.NOTICEDAY.TONotNullString() != "")
                                {
                                    <p class="form-control-static">
                                        @Model.Form.NOTICEDAY_YEAR
                                        <label>年</label>
                                        @Model.Form.NOTICEDAY_MONTH
                                        <label>月</label>
                                    </p>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class='panel panel-info'>
            <div class='panel-heading'>
                <h2 class='panel-title' data-toggle='collapse' data-parent='#srvlistData' href='#srvlistData'>
                    <a>服務經歷(點擊展開)</a>
                </h2>
            </div>
            <div id='srvlistData' class='panel-collapse collapse'>
                <div class='panel-body'>
                    <div class="form-blue">
                        @{
                            var i = 1;
                            foreach (var item in Model.Form.SRVLIST)
                            {
                                var listNum = i - 1;
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">序號</label>
                                    <div class="col-sm-10">
                                        <p id="SEQ_NO" class="form-control-static">@i</p>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務經歷-服務單位名稱<br />(請填全銜)</label>
                                    <div class="col-sm-4">
                                        <input type="text" class="form-control" readonly="readonly" value="@item.SRV_NAM">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務經歷-職稱</label>
                                    <div class="col-sm-4">
                                        <input type="text" class="form-control" id="" readonly="readonly" value="@item.SRV_TITLE">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務經歷-服務年資</label>
                                    <div class="col-sm-10 form-inline">
                                        <div class="date datepicker-outline input-group" tagid="SRV_SYEAR_TW" disabled="disabled">
                                            <input class="datepickerinput" id="SRV_SYEAR" name="SRVLIST[0].SRV_SYEAR" type="hidden" value="">
                                            <input class="form-control" id="SRV_SYEAR_TW" name="SRVLIST[0].SRV_SYEAR_TW" size="10" type="text" value="@item.SRV_SYEAR_AD" readonly>
                                            <span class="input-group-btn"><a class="open-datepicker btn-info btn" role="button"><i class="fa fa-calendar" style="color: white" aria-hidden="true"></i><span class="sr-only">calendar</span></a></span>
                                        </div>
                                        <span>&nbsp;至&nbsp;</span>
                                        <div class="date datepicker-outline input-group" tagid="SRV_EYEAR_TW" disabled="disabled">
                                            <input class="datepickerinput" id="SRV_EYEAR" name="SRVLIST[0].SRV_EYEAR" type="hidden" value="">
                                            <input class="form-control" id="SRV_EYEAR_TW" name="SRVLIST[0].SRV_EYEAR_TW" size="10" type="text" value="@item.SRV_EYEAR_AD" readonly>
                                            <span class="input-group-btn"><a class="open-datepicker btn-info btn" role="button"><i class="fa fa-calendar" style="color: white" aria-hidden="true"></i><span class="sr-only">calendar</span></a></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務證明正本彩色檔</label>
                                    <div class="col-sm-10">
                                        @{
                                            if (Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 1).ToList().Count > 0)
                                            {
                                                @Html.FileDLForTurbo(m => m.Form.SRVLIST[(listNum)].FILE_PICRGB,
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 1).ToList().FirstOrDefault().FILENAME.Split('\\')[3].ToString() + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 1).ToList().FirstOrDefault().APP_ID + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 1).ToList().FirstOrDefault().FILE_NO + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 1).ToList().FirstOrDefault().SRC_NO + "," + i.ToString())

                                            }
                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務單位認可有案證明文件</label>
                                    <div class="col-sm-10">
                                        @{
                                            if (Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 2).ToList().Count > 0)
                                            {
                                                @Html.FileDLForTurbo(m => m.Form.SRVLIST[(listNum)].FILE_SRVPROVE,
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 2).ToList().FirstOrDefault().FILENAME.Split('\\')[3].ToString() + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 2).ToList().FirstOrDefault().APP_ID + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 2).ToList().FirstOrDefault().FILE_NO + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 2).ToList().FirstOrDefault().SRC_NO + "," + i.ToString())
                                            }
                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">勞工保險被保險人投保明細表影本<br />(服務證明為團體或私立機構開具者需附)</label>
                                    <div class="col-sm-10">
                                        @{
                                            if (Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 3).ToList().Count > 0)
                                            {
                                                @Html.FileDLForTurbo(m => m.Form.SRVLIST[(listNum)].FILE_LABOR,
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 3).ToList().FirstOrDefault().FILENAME.Split('\\')[3].ToString() + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 3).ToList().FirstOrDefault().APP_ID + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 3).ToList().FirstOrDefault().FILE_NO + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 3).ToList().FirstOrDefault().SRC_NO + "," + i.ToString())
                                            }
                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務單位立案或法人登記證書影本<br />(服務證明為團體或私立機構開具者需附)</label>
                                    <div class="col-sm-10">
                                        <span>特殊需求:</span>
                                        @item.SRVLST_DEMAND_1
                                        @Html.TextBoxFor(m => m.Form.SRVLIST[(i - 1)].SRVLST_DEMAND_1_TEXT, new { @id = "SRVLST_DEMAND_1_TEXT", @class = "form-control", placeholder = "請輸入特殊需求", @readonly = "readonly" })
                                        @{
                                            if (Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 4).ToList().Count > 0)
                                            {
                                                @Html.FileDLForTurbo(m => m.Form.SRVLIST[(listNum)].FILE_LEGAL,
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 4).ToList().FirstOrDefault().FILENAME.Split('\\')[3].ToString() + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 4).ToList().FirstOrDefault().APP_ID + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 4).ToList().FirstOrDefault().FILE_NO + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 4).ToList().FirstOrDefault().SRC_NO + "," + i.ToString())
                                            }
                                        }
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="main-label col-sm-2" for="">服務單位章程影本<br />(服務證明為團體或私立機構開具者需附)</label>
                                    <div class="col-sm-10">
                                        <span>特殊需求:</span>
                                        @item.SRVLST_DEMAND_2
                                        @Html.TextBoxFor(m => m.Form.SRVLIST[(i - 1)].SRVLST_DEMAND_2_TEXT, new { @id = "SRVLST_DEMAND_2_TEXT", @class = "form-control", placeholder = "請輸入特殊需求", @readonly = "readonly" })
                                        @{
                                            if (Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 5).ToList().Count > 0)
                                            {
                                                @Html.FileDLForTurbo(m => m.Form.SRVLIST[(listNum)].FILE_CHARTER,
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 5).ToList().FirstOrDefault().FILENAME.Split('\\')[3].ToString() + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 5).ToList().FirstOrDefault().APP_ID + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 5).ToList().FirstOrDefault().FILE_NO + "," +
     Model.Form.FILE.Where(x => x.BATCH_INDEX == (i) && x.SRC_NO == 5).ToList().FirstOrDefault().SRC_NO + "," + i.ToString())
                                            }
                                        }
                                    </div>
                                </div>
                                i++;
                            }
                        }
                    </div>
                </div>
            </div>
        </div>
        <!--2-->
        <div class='panel panel-info'>
            <div class='panel-heading'>
                <h2 class='panel-title' data-toggle='collapse' data-parent='#ApplyData' href='#ApplyData'>
                    <a>上傳應附檔案(點擊展開)</a>
                </h2>
            </div>
            <div id='ApplyData' class='panel-collapse collapse'>
                <div class='panel-body'>
                    <div class="form-blue">
                        <div class="form-group">
