            $('#ED_24').removeAttr("style");
        }

        if (($('#Form_FLOW_CD option:selected').val() == "0" || $('#Form_FLOW_CD option:selected').val() == "2") && $('#Form_IsNotice').val() == 'Y') {
            $("#Form_FLOW_CD").attr('disabled', 'disabled');
            $('#btnCaseNo').hide();
            $('#MOHW_CASE_NO').attr("readonly", "readonly");
            $('#btnSave').hide();
        }

        $("#Form_FLOW_CD [value='1']").remove();
        $("#Form_FLOW_CD [value='3']").remove();

        if ($("#Form_ISMEET").val() == "1") {
            $('#ISM_1').hide()
            $('#ISM_2').hide()
        }
        else if ($("#Form_ISMEET").val()=="2") {
            $('#ISM_1').show()
            $('#ISM_2').hide()
        }
        else if ($("#Form_ISMEET").val() == "3") {
            $('#ISM_1').hide()
            $('#ISM_2').show()
        }
    });

    // 地址
    function loadDialogADDRCodeMapName(data) {
        ajaxLoadMore(data.url,
            data.arg,
            function (resp) {
                if (resp === undefined) data.box.val('');
                else {
                    if (resp.data != '') data.box.val(resp.data);
                    else {
                        data.box.val('');
                        data.add.val('');
                        blockAlert(data.msg);
                    }
                }
            });
    };

    function doADDRSelect() {
        var url = '/ZIP_CO';
        var title = '郵遞區號查詢';
        popupWindow({ 'width': 900 },
            url,
            title,
            null,
            function (retData, doc) {
                if (retData != null) {
                    $('#ADDR').val(retData.Id);
                    $('#ADDR_TEXT').val(retData.Name);
                }
            });
    }

    function doADDRNameGet() {
        var code = $('#ADDR');
        var data = {
            'url': '/Ajax/GetCityTown',
            'msg': '查無該郵遞區號!!',
            'arg': { 'CODE': code.val().toUpperCase() },
            'box': $('#ADDR_TEXT'),
            'add': $('#ADDR')
        };
        if ($.trim(data.arg.CODE) == '') { data.box.val(''); $('#ADDR').val('') }
        else {
            code.val(data.arg.CODE);
            loadDialogADDRCodeMapName(data);
        }
    }

    function changeAddDocCheck() {
        if ($('#Form_FLOW_CD option:selected').val() == '0') {
            $('#MOHW_CASE_NO').attr("readonly", "readonly");
            $('#btnCaseNo').attr('disabled', 'disabled');
            $('#notice').hide();
        }
        else if ($('#Form_FLOW_CD option:selected').val() == '4') {
            $('#notice').show();
            $('#notice_ITEM').show();
            $('#notice_content').hide();
        }
        else if ($('#Form_FLOW_CD option:selected').val() == '9') {
            $('#notice').show();
            $('#notice_ITEM').hide();
            $('#remark').text('備註');
            $('#notice_content').show();
        }
        else {
            if ($('#Form_FLOW_CD option:selected').val() == '2') {
                $('#notice').show();
                $('#notice_ITEM').show();
            }
            else {
                $('#notice').hide();
            }

            $('#MOHW_CASE_NO').removeAttr("readonly");
            $('#btnCaseNo').removeAttr('disabled')
        }
    }

    function checkfile() {
        var ValArr = new Array();
        $('input[type="checkbox"][id="chkfile"]:checked').each(function (i) {
            ValArr[i] = this.value;
        });
        $('#Form_FileCheck').val(ValArr.join());
    }

   // 送出
    function doSave() {

        var ValStr = "";

        if (ValStr != "") { blockAlert(ValStr, "提示訊息"); }
        else {
            var data = getFormDataToObject($("form[id=main_form]"));

            ajaxLoadMore('@Url.Action("Save")', data, function (retData) {
                if (retData.status) {
                    blockMessage(retData.message, "提示訊息", function () {
                        var id = $('#Form_APP_ID').val();
                        location.href = "Apply_011003?appid=" + id + "&srvid=011003";
                    });
                }
                else {
                    blockAlert(retData.message, "提示訊息");
                }
            });

        }

    }

    // 取公文號
    function GetMOHW_CASE_NO(UNIT_CD) {
        var APP_ID = $('#Form_APP_ID').val();
        var data = {
            "UNIT_CD": UNIT_CD,
            "APP_ID": APP_ID
        }
        ajaxLoadMore('/AJAX/GetMOHW_CASE_NO', data, function (retData) {
            if (retData.status) {
                $('#MOHW_CASE_NO').val(retData.message)
            }
            else {
                blockAlert(retData.message, "提示訊息");
            }
        });
    }
    // 取得ZIP
    function GetZip(APP_ID) {
        var data = {
            "APP_ID": APP_ID,
            "CASENAME": "社會工作實務經驗年資審查線上申辦"
        }
        var url = '/GetZIP/GetZIP?' + $.param(data);
        location.href = url;
    }


    // 返回
    function doBack() {
        location.href = '@Url.Action("Index", "Case")';
    }
</script>

