@using ES.Models;
@using ES.Models.ViewModels;
@model Apply_012001FormModel

   
@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("Apply", "Apply_012001", FormMethod.Post, htmlAttributes: new { @class = "form-horizontal", id = "main_form", @enctype = "multipart/form-data" }))
{
    <div class="container">
        <div class="col-sm-12">
            <!-- bread  -->
            <div class="breadlink"><i class="fas fa-home"></i><a href="@(Url.Action("Index", "Login"))">首頁</a> ／ <a href="@(Url.Action("Index", "Service"))">申辦服務</a> ／ <a href="#">檔案應用申請</a></div>
            <div class="clearfix"></div>
            <div class="step_map">
                <ul>
                    <li id="form_tab" class="active">填寫申報表件並上傳檔案</li>
                    <li id="form_tab1">預覽申辦表件</li>
                    <li>完成申報</li>
                </ul>
            </div>
            <div class="tab-content" id="myTabContent">
                <!--form-->
                <div id="form" class="tab-pane fade active in">
                    @Html.Partial("Apply")
                </div>
            </div>
        </div>

        <div class="clearfix"></div>

    </div>
}
