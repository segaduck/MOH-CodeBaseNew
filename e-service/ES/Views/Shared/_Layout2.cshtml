@{
    ViewBag.Member = Session["Member"];
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE10" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - 衛生福利部線上申辦系統</title>
    @Styles.Render("~/Content/themes/redmond/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    <script src="@Url.Action("Index", "Home")Scripts/public.js" type="text/javascript"></script>
    @RenderSection("scripts", required: false)
    <script language="javascript" type="text/javascript">

        @if(!String.IsNullOrEmpty(ViewBag.tempMessage)) {
            @Html.Raw("$(function () {")
            @Html.Raw("jAlert(\"")@ViewBag.tempMessage@Html.Raw("\");") 
            @Html.Raw("});")
        }

        @Html.Raw(@ViewBag.Script)
    </script>
</head>
<body style="background-color: #FFF;">
<center>
<div style="width: 770px; text-align: left;">
@RenderBody()
</div>
</center>
</body>
</html>
